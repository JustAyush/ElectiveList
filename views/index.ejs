<% include partials/header.ejs %>
    <div class="container">
        <h2>HomePage</h2>

        <form class="add-player-form" action="/" method="post" enctype="multipart/form-data">
            <div class="form-row">

                <div class="form-group col-md-4">
                    <select id="year" name="year" class="form-control" required>
                      <option selected disabled required>Choose year</option>
                    <% year.forEach((year, index) => { %>
                        <option><%= year %></option>
                    <% }) %>
                  </select>
                </div>

                <div class="form-group col-md-4">
                    <select id="section" name="section" class="form-control" required>
                      <option selected disabled required>Choose section</option>
                        <option>BEX</option>
                        <option>BCT</option>
                  </select>
                </div>

            </div>

            <button type="submit" class="btn btn-primary float-right">Check</button>
        </form>



    </div>
</div>
</body>
</html>
